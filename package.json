{
  "name": "parcel-boilerplate",
  "version": "1.0.0",
  "description": "Simple starter parcel boilerplate for HTML/SCSS projects.",
  "main": "src/js/main.js",
  "repository": "git@github.com:r-valchev/template-parcel.git",
  "author": "Radoslav Valchev <radoslav.valchev.web@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn serve",
    "serve": "parcel src/pages/*.html --open",
    "build": "parcel build src/pages/*.html -d build --public-url ./",
    "eslint": "esw 'src/assets/js/*'",
    "fix": "esw --fix --watch 'src/assets/js/*'"
  },
  "dependencies": {
    "swiper": "^6.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.4.0",
    "eslint-watch": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "posthtml-include": "^1.4.3",
    "sass": "^1.26.9"
  },
  "keywords": [
    "parcel",
    "parcel-bundler",
    "boilerplate",
    "template",
    "sass",
    "scss"
  ],
  "bugs": "https://github.com/r-valchev/template-parcel/issues",
  "private": true
}
